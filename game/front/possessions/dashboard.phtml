<ips:template parameters="$possessionData" />
<div style="background-color: #e6e8eb">
  <div class="ipsColumns">
    <div class="ipsColumn ipsColumn_fluid">
      <div class="ipsType_sectionTitle ipsType_reset cForumTitle">
        <h4 class="ipsDataItem_title"><strong>{$possessionData['nazwa']} (UID: {$possessionData['id']})</strong></h4><br>
      </div>
      <ul class="ipsDataList ipsBox ipsPad">
        <div class="ipsColumn ipsColumn_wide">

          <br/>
          <div class="ipsAreaBackground ipsPad">
            <h1 class="ipsType_sectionTitle ipsType_reset cForumTitle">Informacje o drzwiach</h1>
            <ol class=" ipsGrid ipsGrid_collapsePhone" data-ipsgrid="" data-ipsgrid-equalheights="row">    
              <li class="ipsStreamItem ipsStreamItem_contentBlock ipsAreaBackground_reset ipsPad ipsType_center ipsGrid_span4" style="height: 122px; padding-top: 28px;">	    
                <i class="fa fa-user-o fa-3x ipsType_issue"></i>   
                <div class="ipsStreamItem_container">
                  <div class="ipsStreamItem_header ipsSpacer_top ipsSpacer_half" >
                    <h2 class="ipsType_reset ipsStreamItem_title ipsTruncate ipsTruncate_line">
                      <span class="ipsType_break">{$possessionData['imie_nazwisko']}</span>				
                    </h2>
                    <p class="ipsType_reset ipsType_medium">Właściciel drzwi</p>
                  </div>
                </div> 
              </li>
              <li class="ipsStreamItem ipsStreamItem_contentBlock ipsAreaBackground_reset ipsPad ipsType_center ipsGrid_span4" style="height: 122px; padding-top: 28px;">	    
                <i class="fa fa-dollar fa-3x ipsType_positive"></i>   
                <div class="ipsStreamItem_container">
                  <div class="ipsStreamItem_header ipsSpacer_top ipsSpacer_half" >
                    <h2 class="ipsType_reset ipsStreamItem_title ipsTruncate ipsTruncate_line">
                      <span class="ipsType_break">${$possessionData['cash']}</span>				
                    </h2>
                    <p class="ipsType_reset ipsType_medium">Gotówka w sejfie</p>
                  </div>
                </div> 
              </li>
              <li class="ipsStreamItem ipsStreamItem_contentBlock ipsAreaBackground_reset ipsPad ipsType_center ipsGrid_span4" style="height: 122px; padding-top: 28px;">	    
                <i class="fa fa-ticket fa-3x"></i>   
                <div class="ipsStreamItem_container">
                  <div class="ipsStreamItem_header ipsSpacer_top ipsSpacer_half" >
                    <h2 class="ipsType_reset ipsStreamItem_title ipsTruncate ipsTruncate_line">
                      <span class="ipsType_break">${$possessionData['cost']}</span>				
                    </h2>
                    <p class="ipsType_reset ipsType_medium">Koszt wstępu</p>
                  </div>
                </div> 
              </li>
            </ol>
          </div>
          <br />
          <div class="ipsAreaBackground_light ipsPad">
            <h1 class="ipsType_sectionTitle ipsType_reset cForumTitle">Szczegóły</h1>
            <ol class=" ipsGrid ipsGrid_collapsePhone" data-ipsgrid="" data-ipsgrid-equalheights="row">    
              <li class="ipsStreamItem ipsStreamItem_contentBlock ipsAreaBackground_reset ipsPad ipsType_center ipsGrid_span3" style="height: 122px; padding-top: 28px;">	    
               {{if $possessionData['lock'] == 0 }} 
                         <i class="fa fa-unlock-alt fa-3x ipsType_positive"></i>   
                        {{else}}
                         <i class="fa fa-lock fa-3x ipsType_negative"></i>   
                        {{endelse}}
                        {{endif}}      
                <div class="ipsStreamItem_container">
                  <div class="ipsStreamItem_header ipsSpacer_top ipsSpacer_half" >
                    <h2 class="ipsType_reset ipsStreamItem_title ipsTruncate ipsTruncate_line">
                      <span class="ipsType_break">
                        {{if $possessionData['lock'] == 0 }} 
                        Otwarte
                        {{else}}
                        Zamknięte
                        {{endelse}}
                        {{endif}}                      
                      </span>				
                    </h2>
                    <p class="ipsType_reset ipsType_medium">Status drzwi</p>
                  </div>
                </div> 
              </li>
              <li class="ipsStreamItem ipsStreamItem_contentBlock ipsAreaBackground_reset ipsPad ipsType_center ipsGrid_span3" style="height: 122px; padding-top: 28px;">	    
                {{if $possessionData['garage'] == 1}}
                         <i class="fa fa-car fa-3x ipsType_positive"></i>   
                        {{else}}
                         <i class="fa fa-car fa-3x ipsType_negative"></i>   
                        {{endelse}}
                        {{endif}}      
                <div class="ipsStreamItem_container">
                  <div class="ipsStreamItem_header ipsSpacer_top ipsSpacer_half" >
                    <h2 class="ipsType_reset ipsStreamItem_title ipsTruncate ipsTruncate_line">
                      <span class="ipsType_break">
                        {{if $possessionData['garage'] == 1}}
                        	Tak
                        {{else}}
                        	Nie
                        {{endelse}}
                        {{endif}}
                      </span>				
                    </h2>
                    <p class="ipsType_reset ipsType_medium">Drzwi przejezdne</p>
                  </div>
                </div> 
              </li>
              <li class="ipsStreamItem ipsStreamItem_contentBlock ipsAreaBackground_reset ipsPad ipsType_center ipsGrid_span3" style="height: 122px;">	    
                <div class="ipsStreamItem_container">
                  <i class="fa fa-location-arrow fa-3x" aria-hidden="true"></i>
                  <div class="ipsStreamItem_header ipsSpacer_top ipsSpacer_half">
                    <h2 class="ipsType_reset ipsStreamItem_title ipsTruncate ipsTruncate_line">
                      <span class="ipsType_break" style="font-size: 14px;">
                        <p style="line-height: 1.1; margin: 0;">
                          X: {$possessionData['enterx']}<br/>
                          Y: {$possessionData['entery']}<br/>
                          Z: {$possessionData['enterz']}
                        </p>
                      </span>				
                    </h2>
                    <p class="ipsType_reset ipsType_medium">Pozycja wejścia</p>
                  </div>
                </div> 
              </li>
              <li class="ipsStreamItem ipsStreamItem_contentBlock ipsAreaBackground_reset ipsPad ipsType_center ipsGrid_span3" style="height: 122px;">    
                <i class="fa fa-map-marker fa-3x" aria-hidden="true"></i>
                <div class="ipsStreamItem_container">

                  <div class="ipsStreamItem_header ipsSpacer_top ipsSpacer_half">
                    <h2 class="ipsType_reset ipsStreamItem_title ipsTruncate ipsTruncate_line">
                      <span class="ipsType_break" style="font-size: 14px;">
                        <p style="line-height: 1.1; margin: 0;">
                          X: {$possessionData['exitx']}<br/>
                          Y: {$possessionData['exity']}<br/>
                          Z: {$possessionData['exitz']}
                        </p>			
                        </h2>
                      <p class="ipsType_reset ipsType_medium">Pozycja wyjścia</p>
                      </div>         
                  </div>
                  </li>  
            </ol>
          </div>
          <!-- End of Areabackground -->          
        </div>
      </ul>
    </div>
    <div style="clear: both"></div>
  </div>
</div>