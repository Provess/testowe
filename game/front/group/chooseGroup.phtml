<ips:template parameters="$groupData, $perm, $income " />
<script type="text/javascript" src="uploads/loader.js"></script>
<script type="text/javascript">
  google.charts.load('current', {'packages':['corechart']});
  google.charts.setOnLoadCallback(drawChart);

  function drawChart() {
    var data = google.visualization.arrayToDataTable([
      ['Dzień', 'Zarobek'],
      {{if $income}}
        {{foreach $income as $row}}
        ['{datetime="$row['date']" dateonly="true"}',  {expression="$row['roznica']"}],
        {{endforeach}}
      {{else}}
        [0, 0],                                       
      {{endif}}
    ]);

    var options = {
      title: '',
      hAxis: {title: 'Data',  titleTextStyle: {color: '#333'}},
      vAxis: {minValue: 0}
    };

    var chart = new google.visualization.AreaChart(document.getElementById('chart_div'));
    chart.draw(data, options);
  }
</script>
<div style="background-color: #e6e8eb">
  <div class="saGroupTab ipsColumn_fluid tab_1" style="display: block; top: -20px">     
    <section id="elProfileTabContent" class="ipsBox ipsPad">
      <div class="ipsColumns">
        <div class="ipsColumn ipsColumn_fluid">
          <div class="ipsType_sectionTitle ipsType_reset cForumTitle">
            <h4 class="ipsDataItem_title"><strong>Wykres zarobków</strong></h4><br>
          </div>
          <ul class="ipsDataList ipsBox ipsPad">
            <div class="ipsColumn ipsColumn_wide">
              <div id="chart_div" style="width: 100%; height: 300px;"></div>
            </div>
          </ul>
        </div>
        <div style="clear: both"></div>
      </div>
    </section><br />
